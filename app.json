{
  "expo": {
    "name": "TO-DO LIST",
    "slug": "to-do-list",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/to-do-list.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/to-do-list.png",
      "resizeMode": "contain",
      "imageWidth": 200,
      "imageHeight": 200,
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/to-do-list.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.javiertoussentfis.expobasics",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/to-do-list.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8ac183fb-2152-4ad5-a647-d02913587971"
      }
    },
    "owner": "javiertoussentfis",
    "updates": {
      "url": "https://u.expo.dev/8ac183fb-2152-4ad5-a647-d02913587971"
    }
  }
}